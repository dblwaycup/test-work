<?xml version="1.0"?>
<autotest>
	<control_points> <!-- checked objects iterations -->
		<point>
			<object>none</object> <!-- obj type -->
			<state>2</state> <!-- when we came to this state -->
			<stop>true</stop><!-- autotest stop -->
		</point>
	</control_points>
	<start_auto_play_when_game_open>0</start_auto_play_when_game_open> <!-- if set 1 - the game started auto test, right after starting-->
	<create_new_player_each_start>0</create_new_player_each_start><!-- new player per each game start -->
	<speed_coeff_dt>80.0</speed_coeff_dt> <!-- standart delta time * this coeff -->
	<time_decide>0.1</time_decide> <!-- delta beetwen action. 0.1 - min, else can appears any bugs -->
	<lock_cursor>0</lock_cursor> <!-- if set 1, cursor will be lock to system level, if set 0 then logic level-->
	<cheats_on>1</cheats_on> <!-- cheats -->
	<auto_add_res>1</auto_add_res> <!-- autotest подбирает extra -->
</autotest>