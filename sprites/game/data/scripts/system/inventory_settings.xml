<?xml version="1.0" ?>
<inv_params>
	<tweens size="1">

		<!-- morpho objects -->
		<tween_queue>
			<type>1</type>
			<anchor x="0" y="0" />
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>250</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1.05</param1>
						<param2>1.05</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>220</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param2>0.75</param2>
						<param1>0.75</param1>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>240</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>0.9</param1>
						<param2>0.9</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>300</duration> <!-- time in ms -->
						<type>rotation</type> <!-- scale, position, rotation, alpha -->
						<param1>0.45</param1>
						<param2>0</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>350</duration> <!-- time in ms -->
						<type>rotation</type> <!-- scale, position, rotation, alpha -->
						<param1>-0.30</param1>
						<param2>0</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>350</duration> <!-- time in ms -->
						<type>rotation</type> <!-- scale, position, rotation, alpha -->
						<param1>0.20</param1>
						<param2>0</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>350</duration> <!-- time in ms -->
						<type>rotation</type> <!-- scale, position, rotation, alpha -->
						<param1>0</param1>
						<param2>0</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
			</queue_array>
		</tween_queue>
	
	<!-- inv icon over -->
		<tween_queue>
			<name>inv_icon</name>
			<anchor x="0.5" y="0.5" />	
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>250</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1.3</param1>
						<param2>1.3</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>50</delay>
						<duration>240</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1</param1>
						<param2>1</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>260</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1.2</param1>
						<param2>1.2</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
			</queue_array>
		</tween_queue>
		
	<!-- hide inv image -->
		<tween_queue>
			<type>100</type>
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<duration>450</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>0</param1>
						<param2>0</param2>
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
				<queue size="1">
					<transform>
						<duration>450</duration> <!-- time in ms -->
						<type>alpha</type> <!-- scale, position, rotation, alpha -->
						<param1>0</param1>
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
			</queue_array>
		</tween_queue>

		<!-- show inv image -->
		<tween_queue>
			<type>101</type>
			<start_alpha>0</start_alpha>
			<start_scale>0</start_scale>
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<duration>450</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1</param1>
						<param2>1</param2>
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
				<queue size="1">
					<transform>
						<duration>450</duration> <!-- time in ms -->
						<type>alpha</type> <!-- scale, position, rotation, alpha -->
						<param1>255</param1>
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
			</queue_array>
		</tween_queue>
		
		<!-- включается перед прилетом в инв за время указанное в duration -->
		<tween_queue>
			<type>102</type>
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<delay>50</delay>
						<duration>200</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>0.53</param1>
						<param2>0.53</param2>
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
				<queue size="1">
					<transform>
						<delay>50</delay>
						<duration>200</duration> <!-- time in ms -->
						<type>alpha</type> <!-- scale, position, rotation, alpha -->
						<param1>100</param1>
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
			</queue_array>
		</tween_queue>

		<!-- show back inv -->
		<tween_queue>
			<type>117</type>
			<start_alpha>0</start_alpha>
			<start_scale>0</start_scale>
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>400</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1.1</param1>
						<param2>1.1</param2>
						<param3>1</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
					<transform>
						<delay>0</delay>
						<duration>100</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1</param1>
						<param2>1</param2>
						<param3>1</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>400</duration> <!-- time in ms -->
						<type>alpha</type> <!-- scale, position, rotation, alpha -->
						<param1>255</param1>
						<type_int>ease_inOutQuad</type_int>
					</transform>		
				</queue>
			</queue_array>
		</tween_queue>
		
		<!-- hide inv object -->
		<tween_queue>
			<type>118</type>
			<start_alpha>255</start_alpha>
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>300</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>0</param1>
						<param2>0</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>300</duration> <!-- time in ms -->
						<type>alpha</type> <!-- scale, position, rotation, alpha -->
						<param1>0</param1>
						<type_int>ease_inOutQuad</type_int>
					</transform>		
				</queue>
			</queue_array>
		</tween_queue>
		
		<!-- show inv object -->
		<tween_queue>
			<type>119</type>
			<start_alpha>255</start_alpha>
			<start_scale>0</start_scale>
			<queue_array size = "1">
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>300</duration> <!-- time in ms -->
						<type>scale</type> <!-- scale, position, rotation, alpha -->
						<param1>1</param1>
						<param2>1</param2>
						<param3>0</param3> <!-- is local -->
						<type_int>ease_inOutSin</type_int>
					</transform>
				</queue>
				<queue size="1">
					<transform>
						<delay>0</delay>
						<duration>400</duration> <!-- time in ms -->
						<type>alpha</type> <!-- scale, position, rotation, alpha -->
						<param1>240</param1>
						<type_int>ease_inOutQuad</type_int>
					</transform>		
				</queue>
			</queue_array>
		</tween_queue>
	
	</tweens>
	
	<multi_params>	
		<param>
			<name>1</name><!-- id -->
			<params>
				<param>
					<actions>
						<action>1</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
					</points>
				</param>
				<param>
					<actions>
						<action>0.7</action>
						<action>0.7</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
						<delta x="24" y="24"/>
					</points>
				</param>
				<param>
					<actions>
						<action>0.5</action>
						<action>0.5</action>
						<action>0.5</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
						<delta x="40" y="0"/>
						<delta x="20" y="40"/>
					</points>
				</param>
				<param>
					<actions>
						<action>0.5</action>
						<action>0.5</action>
						<action>0.5</action>
						<action>0.5</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
						<delta x="40" y="0"/>
						<delta x="0" y="40"/>
						<delta x="40" y="40"/>
					</points>
				</param>
			</params>
		</param>
		<param>
			<name>2</name><!-- id -->
			<params>
				<param>
					<actions>
						<action>1</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
					</points>
				</param>
				<param>
					<actions>
						<action>0.75</action>
						<action>0.75</action>
					</actions>
					<points>
						<delta x="-5" y="5"/>
						<delta x="24" y="0"/>
					</points>
				</param>
				<param>
					<actions>
						<action>0.5</action>
						<action>0.5</action>
						<action>0.5</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
						<delta x="40" y="0"/>
						<delta x="20" y="40"/>
					</points>
				</param>
				<param>
					<actions>
						<action>0.5</action>
						<action>0.5</action>
						<action>0.5</action>
						<action>0.5</action>
					</actions>
					<points>
						<delta x="0" y="0"/>
						<delta x="40" y="0"/>
						<delta x="0" y="40"/>
						<delta x="40" y="40"/>
					</points>
				</param>
			</params>
		</param>

		<!-- параметры для эффекта подбора предмета в инвентарь -->
		<param>
			<name>ObjectToInventory_hideDefault</name><!-- исчезновения взятого обьекта -->
			<tween_id>100</tween_id><!-- ID твина исчезновения взятого обьекта -->
			<particle>take_object</particle>
		</param>
		<param>
			<name>ObjectToInventory_flyStartDefault</name><!-- полет предмета в инвентарь, стартовый твин -->
			<tween_id>101</tween_id><!-- ID твина появления спрайта, который полетит  -->
			<fly_start_delay>120</fly_start_delay><!-- задержка перед началом полета -->
			<fly_time>800</fly_time><!-- сколько будем лететь -->
			<fly_tween_type>ease_inSin</fly_tween_type><!-- тип твина полета -->
			<start_fly_particle>get_object</start_fly_particle><!-- имя партикла -->
			<params>
				<param>
					<name>CurvePoints</name>
					<params>
						<param>
							<radius_max>200</radius_max>
							<vec_val1 x="0" y="0" />
						</param>
					</params>
				</param>
			</params>
		</param>
		<param>
			<name>ObjectToInventory_flyEndDefault</name><!-- полет предмета в инвентарь, конечный твин -->
			<tween_id>102</tween_id><!-- ID твина полета предмета в инвентарь -->
		</param>
	</multi_params>
	
</inv_params>