			<dialog_name>collectibles_info_panel</dialog_name>
			<text_ctrl>text_title</text_ctrl>
			<text_token>extra_magic_bear_title</text_token>
			<text_ctrl2>text_desc</text_ctrl2>
			<text_token2>extra_magic_bear_desc</text_token2>
			<tooltip>Dialog_1|extra_magic_bear_title</tooltip>
			<image_ctrl>image</image_ctrl>
			<image_name>magic_bear_big</image_name>
